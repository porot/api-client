{
    "name": "CareerAdvice",
    "apiVersion": "2014-01-08",
    "description": "Career advice service",
    "path": "/career/advice",
    "operations": {
        "GetTopics": {
            "httpMethod": "GET",
            "uri": "topics",
            "summary": "Returns a list of career advice topics",
            "type" : "primitive",
            "class": "Porot\\Api\\CareerAdvice\\CommandTopics"
        },
        "GetTopic": {
            "httpMethod": "GET",
            "uri": "topic/",
            "summary": "Returns a career advice topic",
            "type" : "primitive",
            "class": "Porot\\Api\\CareerAdvice\\CommandTopic"
        },
        "GetQuestion": {
            "httpMethod": "GET",
            "uri": "question/",
            "summary": "Returns a career advice question",
            "type" : "primitive",
            "class": "Porot\\Api\\CareerAdvice\\CommandQuestion"
        },
        "GetRandom": {
            "httpMethod": "GET",
            "uri": "random",
            "summary": "Returns a random career advice question",
            "type" : "primitive",
            "class": "Porot\\Api\\CareerAdvice\\CommandRandom"
        }
    }
}
